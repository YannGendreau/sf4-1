{#
    on hérite du template "base.html.twig"
    permet de récup la structure HTML, mais également de remplacer le contenu des blocks

#}

{% extends "base.html.twig" %}

{#on écrase le bloc titre#}
{% block title%}
    Bienvenue !
{% endblock%}

{# On écrase le bloc body #}
{% block body %}
    <p>
        Page d'accueil<br>
        Pseudo: {{  pseudo }},

        <ul>
        {# foreach ($liste as $element#}
        {% for element in liste %}
        <li>{{ element }}</li>
        {% endfor %}
        </ul>

    </p>
{% endblock %}